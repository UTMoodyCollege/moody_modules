<div class="impact-fact-items {{ style }} {{ col_number }}">
  {% for item in impact_items %}
    <div class="impact-fact">
      {% if item.headline %}
        <h3 class="if-headline">{{ item.headline }}</h3>
      {% endif %}
      {% if item.subheadline %}
        <h4 class="if-subheadline">{{ item.subheadline }}</h4>
      {% endif %}
    </div>
  {% endfor %}
</div>
